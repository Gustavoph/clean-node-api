{
  "name": "clean-node-api",
  "version": "1.2.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -L ./dist/main/server.js",
    "build": "rimraf dist && tsc",
    "up": "npm run build && docker compose up -d",
    "down": "docker compose down",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm test -- --watchAll -c jest-unit.config.ts",
    "test:integration": "npm test -- --watchAll -c jest-integration.config.ts",
    "test:ci": "npm test -- --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "express": "^4.18.2",
    "git-commit-msg-linter": "^5.0.4",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "nodemon": "^3.0.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.2.0",
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.2.29",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.7",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20.x"
  }
}
